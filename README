listing.

% Treat each term as a variable and exit on the shortest (number of V) possible match.

q(Tree, [param, of, company], []);
q(Tree, [param, of|X], []), X = [V];
q(Tree, [param|X], []), X = [V, company];
q(Tree, X, []), X = [V, of, company].

q(Tree, [param, of, company], []);
q(Tree, [param, of, A], []);
q(Tree, [param, A, company], []);
q(Tree, [A, of, company], []).