<!doctype html>
<html>
    <head>
        <title>InterMine Natural</title>

        <link rel="stylesheet" type="text/css" href="/css/reset.css" />
        <link rel="stylesheet" type="text/css" href="/css/cargo.less.css">

        <script src="/js/jquery.min.js"></script>
    </head>
    <body>
        <header>
            <div class="wrapper">
                <nav>
                    <ul>
                        <li><a>One</a></li>
                        <li><a>Two</a></li>
                    </ul>
                </nav>

                <div id="logo"></div>
            </div>
        </header>

        <fieldset id="input">
            <div class="wrapper">
                <p>Try these:</p>
                <ul>
                    <li><a>name of employee of company</a></li>
                </ul>

                <form>
                    <input type="text" />
                    <input type="submit" value="Go" />
                </form>
            </div>
        </fieldset>

        <div class="wrapper">
            <aside id="assumptions">
                <ul>
                    <li>
                        <h3><span>pparg</span> is a &hellip;</h3>
                        <ul class="options">
                            <li class="active">gene</li>
                            <li><a>protein</a></li>
                        </ul>
                    </li>
                    <li>
                        <ul class="options">
                            <li class="active">human gene</li>
                            <li><a>mouse gene</a></li>
                            <li><a>rat gene</a></li>
                            <li><a>cow gene</a></li>
                            <li><a>zebrafish gene</a></li>
                            <li><a>dog gene</a></li>
                            <li><a>chimpanzee gene</a></li>
                            <li><a>chicken gene</a></li>
                        </ul>
                    </li>
                </ul>
            </aside>

            <article id="results">

                <ol>
                    <li>
                        <div class="position">1</div>
                        <table class="query">
                            <tr>
                                <td>
                                    <div class="box">
                                        <div class="class">company</div>
                                        <table class="fields">
                                            <tr>
                                                <td class="selected type1">employees</td>
                                                <td class="selected type2">CEO</td>
                                                <td>bank</td>
                                                <td>VAT</td>
                                            </tr>
                                        </table>
                                    </div>
                                </td>
                                <td>
                                    <div class="box type1">
                                        <div class="class">employee</div>
                                        <table class="fields">
                                            <tr>
                                                <td class="selected">name <span>= "Radek"</span></td>
                                                <td>salary</td>
                                            </tr>
                                        </table>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td></td>
                                <td>
                                    <div class="box type2">
                                        <div class="class">CEO</div>
                                        <table class="fields">
                                            <tr>
                                                <td>name</td>
                                                <td class="selected">salary</td>
                                            </tr>
                                        </table>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </li>
                    <li>
                        <div class="position">2</div>
                        <table class="query">
                            <tr>
                                <td>
                                    <div class="box">
                                        <div class="class">company</div>
                                        <table class="fields">
                                            <tr>
                                                <td>employees</td>
                                                <td>CEO</td>
                                                <td class="selected type3">bank</td>
                                                <td>VAT</td>
                                            </tr>
                                        </table>
                                    </div>
                                </td>
                                <td>
                                    <div class="box type3">
                                        <div class="class">bank</div>
                                        <table class="fields">
                                            <tr>
                                                <td class="selected">name</td>
                                                <td>number</td>
                                            </tr>
                                        </table>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </li>
                </ol>

            </article>
        </div>
    </body>
</html>